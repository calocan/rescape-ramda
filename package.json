{
  "name": "rescape-ramda",
  "version": "1.4.4",
  "description": "Ramda utilities for the Rescape platform",
  "homepage": "https://www.rescapes.net/",
  "main": "build/index.js",
  "scripts": {
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "build-publish-patch": "npm run build && npm run test && npm version patch && npm run publish-patch",
    "build-publish-minor": "npm run build && npm run test && npm version minor && npm run publish-minor",
    "publish-patch": "npm version patch && npm publish",
    "publish-minor": "npm version minor && npm publish"
  },
  "repository": "git@github.com:calocan/rescape-ramda.git",
  "author": "Andy Likuski",
  "license": "MIT",
  "dependencies": {
    "data.maybe": "^1.2.2",
    "data.task": "^3.1.1",
    "fs-extra": "^5.0.0",
    "ramda": "^0.24.1",
    "ramda-fantasy": "^0.8.0",
    "ramda-maybe": "^0.18.0",
    "util": "^0.10.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "6.25.0",
    "babel-loader": "7.1.1",
    "babel-plugin-transform-es2015-modules-commonjs-simple": "^6.7.4",
    "babel-preset-es2015": "6.24.1",
    "eslint": "^4.6.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-jest": "^21.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "jest": "^20.0.4",
    "jsdom": "^11.1.0",
    "react-dev-utils": "^5.0.0",
    "rimraf": "^2.6.1",
    "webpack": "^3.5.4"
  },
  "jest": {
    "modulePaths": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ]
    ],
    "sourceMaps": true,
    "plugins": [
      [
        "transform-es2015-modules-commonjs-simple",
        {
          "noMangle": true
        }
      ]
    ]
  }
}
